{
  "fmt": {
    "options": {
      "indentWidth": 2,
      "lineWidth": 80,
      "semiColons": false
    }
  },
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "tasks": {
    "dev": "deno bundle --watch ./app.ts ./app.js"
  }
}
